#include <stdio.h>

int main() {
    int matrix[4][4];
  
    printf("Enter elements of 4x4 matrix:\n");

    for(i = 0; i < 4; i++) {
        for(j = 0; j < 4; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }

    printf("\nMatrix with position:\n");
    for(int i = 0; i < 4; i++) {
        for(int j = 0; j < 4; j++) {
            printf("[%d,%d]=%d\t", i+1, j+1, matrix[i][j]);
       }
       printf("\n"); 
    }

    return 0;
}



//OUTPUT
Position (0,0): 1
Position (0,1): 2
Position (0,2): 3
Position (0,3): 4
Position (1,0): 5
...


   
